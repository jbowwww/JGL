using System;

namespace JGL
{
	/// <summary>
	/// Engine.
	/// </summary>
	public static class Engine
	{
		/// <summary>
		/// Init this instance.
		/// </summary>
		public static void Init()
		{

		}

		/// <summary>
		/// Quit this instance.
		/// </summary>
		public static void Quit()
		{
			JGL.Resource.Resource.StopLoadThread();
			JGL.Debugging.AutoTraceSource.StopTraceThread();
		}
	}
}

